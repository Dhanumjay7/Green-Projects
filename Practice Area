var inactiveAgents = getInactiveAgentsFromPDFAndCOMM();

if (inactiveAgents.indexOf(agentCode) !== -1) {
    rows = rows.concat(markAllAsInactive(results, agentCode));
} else {
    rows = rows.concat(checkMissingACS(results));
    rows = rows.concat(checkNonVoid(results));
}

function markAllAsInactive(results, agentCode) {
    return results.map(function (r) {
        return r.recordType + ':' +
               r.id + ':' +
               r.getValue('tranid') + ':' +
               r.getText('entity') + ':' +
               'Inactive Agent ID-' + agentCode;
    });
}
