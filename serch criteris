        /*
        Function Name : Map
        Purpose       : Delete the records
        */
        function Map(context) {

            var recId = context.value;

            try {
                record.delete({

                    type: record.Type.INVOICE,
                    id: recId,
                });

            } catch (err) {
                log.error("Map", err, recId);
            }
        }

        return {
            getInputData: GetInputData,
            map: Map
        };
    });
